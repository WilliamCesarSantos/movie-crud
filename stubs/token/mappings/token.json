{
  "request": {
    "method": "POST",
    "url": "/api/token"
  },
  "response": {
    "status": 201,
    "jsonBody": {
      "access_token": "{{randomValue length=150 type='ALPHANUMERIC'}}",
      "refresh_token": "{{randomValue length=15 type='ALPHANUMERIC'}}",
      "expires_in": "{{randomValue length=10 type='NUMERIC'}}",
      "issued_at": "{{now timezone='America/Sao_Paulo'}}"
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}